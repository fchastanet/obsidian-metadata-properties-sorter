{
  "name": "metaflow",
  "version": "1.0.0",
  "description": "Automated metadata workflow management for Obsidian - sort properties, insert missing fields, and execute custom default value scripts with MetadataMenu and Templater integration",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "debug": "node esbuild.config.mjs debug",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "sync": "node -e \"import('./scripts/sync.mjs').then(m => m.syncFiles())\"",
    "version": "node scripts/version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "metadata",
    "frontmatter",
    "workflow",
    "automation",
    "metadatamenu",
    "templater"
  ],
  "author": "François Chastanet",
  "license": "MIT",
  "funding": {
    "type": "coffeescript",
    "url": "https://coff.ee/fchastanetl"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "builtin-modules": "5.0.0",
    "esbuild": "0.25.8",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "obsidian": "latest",
    "ts-jest": "^29.4.0",
    "tslib": "2.8.1",
    "typescript": "^4.8"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.9",
    "ace-builds": "^1.43.2",
    "js-yaml": "^4.1.0"
  },
  "cspell": {
    "version": "0.2",
    "ignorePaths": [],
    "dictionaryDefinitions": [],
    "dictionaries": [],
    "words": [
      "Chastanet",
      "François",
      "frontmatter",
      "metadatamenu",
      "metaflow",
      "Templater"
    ],
    "ignoreWords": [],
    "import": []
  }
}
