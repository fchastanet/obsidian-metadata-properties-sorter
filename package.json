{
  "name": "metaflow",
  "version": "1.0.0",
  "description": "Automated metadata workflow management for Obsidian - sort properties, insert missing fields, and execute custom default value scripts with MetadataMenu and Templater integration",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "debug": "node esbuild.config.mjs debug",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "sync": "node -e \"import('./scripts/sync.mjs').then(m => m.syncFiles())\"",
    "version": "node scripts/version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "metadata",
    "frontmatter",
    "workflow",
    "automation",
    "metadatamenu",
    "templater"
  ],
  "author": "Fran√ßois Chastanet",
  "license": "MIT",
  "funding": {
    "type": "coffeescript",
    "url": "https://coff.ee/fchastanetl"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "jest": "^30.0.5",
    "obsidian": "latest",
    "ts-jest": "^29.4.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.9",
    "js-yaml": "^4.1.0"
  },
  "cspell": {
    "version": "0.2",
    "ignorePaths": [],
    "dictionaryDefinitions": [],
    "dictionaries": [],
    "words": [
      "frontmatter",
      "Templater",
      "metaflow"
    ],
    "ignoreWords": [],
    "import": []
  }
}
